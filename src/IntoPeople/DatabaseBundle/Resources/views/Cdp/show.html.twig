{% extends '::base.html.twig' %} {% block body -%}
	<h2>{{ "cdp.show.title" | trans }}</h2>

	{% set template = entity.cdptemplate %}

	<div class="row">
		<div class="col-md-12">

			<h3>Personalia</h3>

			<div class="row">

				<div class="col-md-6">

					<table class="table borderless">

						<tr>
							<th>Name:
							<th>
							<td>{{ app.user.firstname }} {{ app.user.lastname }}</td>
						</tr>

						{% if app.user.supervisor != null %}
							<tr>
								<th>Supervisor:
								<th>
								<td>{{ app.user.supervisor.firstname }} {{ app.user.supervisor.lastname }}</td>
							</tr>
						{% endif %}

						<tr>
							<th>Date discussion:
							<th>
							<td></td>
						</tr>

					</table>

				</div>

				<div class="col-md-6">

					<table class="table borderless">

						<tr>
							<th>Team:<th>
							<td></td>
						</tr>


						<tr>
							<th>Functie:<th>
							<td></td>
						</tr>

					</table>

				</div>

			</div>



		</div>
	</div>


	{% if entity.formstatus.id > 3 %}

		<div class="row">
			<div class="col-md-12">

				<h3>Comments supervisor</h3>

				{{ entity.commentSup }}

			</div>
		</div>

	{% endif %}

	{% if entity.formstatus.id > 5 %}

		<div class="row">
			<div class="col-md-12">

				<h3>Feedback HR</h3>

				{{ entity.feedbackHR }}

			</div>
		</div>

	{% endif %}

	<div class="row">
		<div class="col-md-12">

			<h3>{{ template.selfassesment }}</h3>

			<p class="">{{ template.selfassesmentDescription }}</p>


			<table class="table borderless">
				<tr>
					<th style="width: 20%">Talents</th>
					<th style="width: 20%">Challenges</th>
					<th style="width: 20%">How</th>
					<th style="width: 20%">Success</th>
					<th style="width: 20%">Needs</th>
				</tr>

				<tr>
					<td>
						<p>{{ entity.coreQuality1 }}</p>
						<p>{{ entity.coreQuality1Why }}</p>
					</td>
					<td>
						<p>{{ entity.challenge1 }}</p>
						<p>{{ entity.challengeWhy1 }}</p>
					</td>
					<td>{{ entity.challengeHow1 }}</td>
					<td>{{ entity.challengeSuccess1 }}</td>
					<td>{{ entity.challengeNeeds1 }}</td>
				</tr>

				<tr>
					<td>
						<p>{{ entity.coreQuality2 }}</p>
						<p>{{ entity.coreQuality2Why }}</p>
					</td>
					<td>
						<p>{{ entity.challenge2 }}</p>
						<p>{{ entity.challengeWhy2 }}</p>
					</td>
					<td>{{ entity.challengeHow2 }}</td>
					<td>{{ entity.challengeSuccess2 }}</td>
					<td>{{ entity.challengeNeeds2 }}</td>
				</tr>

				<tr>
					<td>
						<p>{{ entity.coreQuality3 }}</p>
						<p>{{ entity.coreQuality3Why }}</p>
					</td>
					<td>
						<p>{{ entity.challenge3 }}</p>
						<p>{{ entity.challengeWhy3 }}</p>
					</td>
					<td>{{ entity.challengeHow3 }}</td>
					<td>{{ entity.challengeSuccess3 }}</td>
					<td>{{ entity.challengeNeeds3 }}</td>
				</tr>

				<tr>
					<td>
						<p>{{ entity.coreQuality4 }}</p>
						<p>{{ entity.coreQuality4Why }}</p>
					</td>
					<td>
						<p>{{ entity.challenge4 }}</p>
						<p>{{ entity.challengeWhy4 }}</p>
					</td>
					<td>{{ entity.challengeHow4 }}</td>
					<td>{{ entity.challengeSuccess4 }}</td>
					<td>{{ entity.challengeNeeds4 }}</td>
				</tr>

				<tr>
					<td>
						<p>{{ entity.coreQuality5 }}</p>
						<p>{{ entity.coreQuality5Why }}</p>
					</td>
					<td>
						<p>{{ entity.challenge5 }}</p>
						<p>{{ entity.challengeWhy5 }}</p>
					</td>
					<td>{{ entity.challengeHow5 }}</td>
					<td>{{ entity.challengeSuccess5 }}</td>
					<td>{{ entity.challengeNeeds5 }}</td>
				</tr>

			</table>

		</div>
	</div>

	<div class="row">
		<div class="col-md-12">


			<h3>Development Needs for your Job</h3>

			<p>{{ template.developmentneeds }}</p>

			<table class="table borderless">

				<thead>
				<tr>
					<th style="width: 22%" colspan="2">{{ template.whatWhy }}</th>
					<th style="width: 12%">{{ template.how }}</th>
					<th style="width: 12%">{{ template.success }}</th>
					<th style="width: 12%">{{ template.needs }}</th>
				</tr>
				</thead>

				<tbody>

				<tr>
					<td colspan="2">{{ template.whatWhyDescription }}</td>
					<td>{{ template.howDescription }}</td>
					<td>{{ template.successDescription }}</td>
					<td>{{ template.needsDescription }}</td>
				</tr>

				<tr>
					<td colspan="5"><h5 class="text-center">Job specific tasks and
							responsibilities</h5></td>
				</tr>


				<tr>
					<td colspan="2">
						{{ entity.developmentneeds.task1 }}</td>
					<td>{{ entity.taskhow1 }}</td>
					<td>{{ entity.tasksucces1 }}</td>
					<td>{{ entity.taskneeds1 }}</td>
				</tr>

				<tr>
					<td colspan="2">{{ entity.developmentneeds.task2 }}</td>
					<td>{{ entity.taskhow2 }}</td>
					<td>{{ entity.tasksucces2 }}</td>
					<td>{{ entity.taskneeds2 }}</td>
				</tr>

				<tr>
