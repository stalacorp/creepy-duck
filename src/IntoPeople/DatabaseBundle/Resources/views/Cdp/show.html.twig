{% extends '::base.html.twig' %} {% block body -%}
<!-- BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM-->
<div id="portlet-config" class="modal hide">
	<div class="modal-header">
		<button data-dismiss="modal" class="close" type="button"></button>
		<h3>Widget Settings</h3>
	</div>
	<div class="modal-body">Widget settings form goes here</div>
</div>
<div class="clearfix"></div>
<div class="container">
	<div class="content">

		<div class="page-title">
			<i class="icon-custom-left"></i>
			<h3>
				<span class="semi-bold">Career development plan</span>
			</h3>
		</div>
		
		{% set template = entity.cdptemplate %}
		
		<div class="row">
			<div class="col-md-12">
				
					   <h3>Personalia</h3>
					   
					   
					   <table class="table table-bordered margin-top-30">
					   
					       <tr>
					           <th>Name:<th>
					           <td>{{ app.user.firstname }} {{ app.user.lastname }}</td>
					       </tr>
					       
					       {% if app.user.supervisor != null %}
					       <tr>
					           <th>Supervisor:<th>
					           <td>{{ app.user.supervisor.firstname }} {{ app.user.supervisor.lastname }}</td>
					       </tr>
					       {% endif %}
					       
					       <tr>
					           <th>Date discussion:<th>
					           <td></td>
					       </tr>
					   
					   </table>					

			</div>
		</div>

		
		<div class="row">
			<div class="col-md-8">


						<h3 class="margin-top-50">{{ template.selfassesment }}</h3>

						<p></p>

						
						<table class="table table-bordered no-more-tables margin-top-50">
							<tr>
								<th style="width: 50%">{{ template.talents }}</th>
								<th style="width: 50%">{{ template.talentsDescription }}</th>
							</tr>

							<tr>

								<td>{{ entity.talent1 }}</td>

								<td>{{ entity.talentwhy1 }}</td>
							</tr>

							<tr>

								<td>{{ entity.talent2 }}</td>

								<td>{{ entity.talentwhy2 }}</td>
							</tr>

							<tr>

								<td>{{ entity.talent3 }}</td>

								<td>{{ entity.talentwhy3 }}</td>
							</tr>

							<tr>

								<td>{{ entity.talent4 }}</td>

								<td>{{ entity.talentwhy4 }}</td>
							</tr>

							<tr>

								<td>{{ entity.talent5 }}</td>

								<td>{{ entity.talentwhy5 }}</td>
							</tr>

						</table>



						<table class="table table-bordered no-more-tables margin-top-50">
							<tr>
								<th style="width: 50%">{{ template.challenges }}</th>
								<th style="width: 50%">{{ template.challengesDescription }}</th>
							</tr>

							<tr>

								<td>{{ entity.challenge1 }}</td>

								<td>{{ entity.challengewhy1 }}</td>
							</tr>

							<tr>

								<td>{{ entity.challenge2 }}</td>

								<td>{{ entity.challengewhy2 }}</td>
							</tr>

							<tr>

								<td>{{ entity.challenge3 }}</td>

								<td>{{ entity.challengewhy3 }}</td>
							</tr>

							<tr>

								<td>{{ entity.challenge4 }}</td>

								<td>{{ entity.challengewhy4 }}</td>
							</tr>

							<tr>

								<td>{{ entity.challenge5 }}</td>

								<td>{{ entity.challengewhy5 }}</td>
							</tr>

						</table>
						</div>
						</div>

						<div class="row margin-top-50">
			             <div class="col-md-12">
						
						<h3>Development Needs for your Job</h3>

						<p>{{ template.developmentneeds }}</p>

						<table class="table table-bordered no-more-tables margin-top-50">

							<thead>
								<tr>
									<th style="width: 22%" colspan="2">{{ template.whatWhy }}</th>
									<th style="width: 12%">{{ template.how }}</th>
									<th style="width: 12%">{{ template.success }}</th>
									<th style="width: 12%">{{ template.needs }}</th>
								</tr>
							</thead>

							<tbody>

								<tr>
									<td colspan="2"><span class="muted">Write down the actions you will take this year,
										to further develop yourself, in your job. What do you want to
										do more and/or what are new things you want to take up. Why
										(what motivates you)?</span></td>
									<td><span class="muted">How will you do this?</span></td>
									<td><span class="muted">How will you know you succeeded?</span></td>
									<td><span class="muted">What help do you need (training, knowledge sharing, books,
										tools, etc.)?</span></td>
								</tr>

								<tr>
									<td colspan="5"><h5 class="text-left">Job specific tasks and
											responsibilities</h5></td>
								</tr>


								<tr>
									<td colspan="2">{{ entity.developmentneeds.task1}}</td>
									<td>{{ entity.taskhow1 }}</td>
									<td>{{ entity.tasksucces1 }}</td>
									<td>{{ entity.taskneeds1 }}</td>
								</tr>

								<tr>
									<td colspan="2">{{ entity.developmentneeds.task2}}</td>
									<td>{{ entity.taskhow2 }}</td>
									<td>{{ entity.tasksucces2 }}</td>
									<td>{{ entity.taskneeds2 }}</td>
								</tr>

								<tr>
									<td colspan="2">{{ entity.developmentneeds.task3}}</td>
									<td>{{ entity.taskhow3 }}</td>
									<td>{{ entity.tasksucces3 }}</td>
									<td>{{ entity.taskneeds3 }}</td>
								</tr>

								<tr>
									<td colspan="2">{{ entity.developmentneeds.task4}}</td>
									<td>{{ entity.taskhow4 }}</td>
									<td>{{ entity.tasksucces4 }}</td>
									<td>{{ entity.taskneeds4 }}</td>
								</tr>

								<tr>
									<td colspan="2">{{ entity.developmentneeds.task5 }}</td>
									<td>{{ entity.taskhow5 }}</td>
									<td>{{ entity.tasksucces5 }}</td>
									<td>{{ entity.taskneeds5 }}</td>
								</tr>

								<tr>
									<td colspan="5"><h5 class="text-left">Job specific skills and
											competencies</h5></td>
								</tr>

								<tr>
									<th style="width: 22%" colspan="2">{{ template.whatWhy }}</th>
									<th style="width: 12%">{{ template.how }}</th>
									<th style="width: 12%">{{ template.success }}</th>
									<th style="width: 12%">{{ template.needs }}</th>
								</tr>


								<tr>
									<td colspan="2">{{ entity.developmentneeds.skill1 }}</td>
									<td>{{ entity.skillshow1 }}</td>
									<td>{{ entity.skillssuccess1 }}</td>
									<td>{{ entity.skillsneeds1 }}</td>
								</tr>

								<tr>
									<td colspan="2">{{ entity.developmentneeds.skill2 }}</td>
									<td>{{ entity.skillshow2 }}</td>
									<td>{{ entity.skillssucces2 }}</td>
									<td>{{ entity.skillsneeds2 }}</td>
								</tr>

								<tr>
									<td colspan="2">{{ entity.developmentneeds.skill3 }}</td>
									<td>{{ entity.skillshow3 }}</td>
									<td>{{ entity.skillssucces3 }}</td>
									<td>{{ entity.skillsneeds3 }}</td>
								</tr>

								<tr>
									<td colspan="2">{{ entity.developmentneeds.skill4 }}</td>
									<td>{{ entity.skillshow4 }}</td>
									<td>{{ entity.skillssucces4 }}</td>
									<td>{{ entity.skillsneeds4 }}</td>
								</tr>

								<tr>
									<td colspan="2">{{ entity.developmentneeds.skill5 }}</td>
									<td>{{ entity.skillshow5 }}</td>
									<td>{{ entity.skillssucces5 }}</td>
									<td>{{ entity.skillsneeds5 }}</td>
								</tr>


								<tr>
									<td colspan="5"><h5 class="text-left">Organization
											competencies</h5></td>
								</tr>

								<tr>
									<th style="width: 22%" colspan="2">{{ template.whatWhy }}</th>
									<th style="width: 12%">{{ template.how }}</th>
									<th style="width: 12%">{{ template.success }}</th>
									<th style="width: 12%">{{ template.needs }}</th>
								</tr>


								<tr>
									<td>{{ entity.developmentneeds.organizationcompetence1 }}</td>
									<td>{{ entity.developmentneeds.organizationdescription1 }}</td>

									<td>{{ entity.organizationhow1 }}</td>
									<td>{{ entity.organizationsuccess1 }}</td>
									<td>{{ entity.organizationneeds1 }}</td>
								</tr>

								<tr>
									<td>{{ entity.developmentneeds.organizationcompetence2 }}</td>
									<td>{{ entity.developmentneeds.organizationdescription2 }}</td>

									<td>{{ entity.organizationhow2 }}</td>
									<td>{{ entity.organizationsuccess2 }}</td>
									<td>{{ entity.organizationneeds2 }}</td>
								</tr>

								<tr>
									<td>{{ entity.developmentneeds.organizationcompetence3 }}</td>
									<td>{{ entity.developmentneeds.organizationdescription3 }}</td>

									<td>{{ entity.organizationhow3 }}</td>
									<td>{{ entity.organizationsuccess3 }}</td>
									<td>{{ entity.organizationneeds3 }}</td>
								</tr>

								<tr>
									<td>{{ entity.developmentneeds.organizationcompetence4 }}</td>
									<td>{{ entity.developmentneeds.organizationdescription4 }}</td>

									<td>{{ entity.organizationhow4 }}</td>
									<td>{{ entity.organizationsuccess4 }}</td>
									<td>{{ entity.organizationneeds4 }}</td>
								</tr>

								<tr>
									<td>{{ entity.developmentneeds.organizationcompetence5 }}</td>
									<td>{{ entity.developmentneeds.organizationdescription5 }}</td>

									<td>{{ entity.organizationhow5 }}</td>
									<td>{{ entity.organizationsuccess5 }}</td>
									<td>{{ entity.organizationneeds5 }}</td>
								</tr>


							</tbody>


						</table>
						
						</div></div>
						
						<div class="row margin-top-50">
			             <div class="col-md-8">


						<h3>{{ template.careerObjectives }}</h3>

						<p>{{ template.careerObjectivesQuestion }}</p>

						<p>{{ entity.careerobjective }}</p>


						<h3>{{ template.additionalInformation }}</h3>

						<p>{{ template.additionalInformationQuestion }}</p>
						
						<p>{{ entity.additionalinformation }} </p>
						
						</div>
						</div>

						{% if entity.formstatus.id > 3 %}
						
						<div class="row margin-top-50">
			             <div class="col-md-6">
						
						<h3>Comments supervisor</h3>
						
						{{ entity.commentSup }}
						
						</div>
						</div>
						
						{% endif %}
						
						{% if entity.formstatus.id > 5 %}
						
						<div class="row margin-top-50">
			             <div class="col-md-6">
						
						<h3>Feedback HR</h3>
						
						{{ entity.feedbackHR }}
						
						</div>
						</div>
						
						{% endif %}
									        
										  							
						{% block comment %}
                        {% endblock %}

             
                        
					    <p class="margin-top-20"><a href="{{ app.request.headers.get('referer') }}" class="btn btn-default btn-cons" type="button">Back</a></p>
							
					
                        
			</div>
		</div>
	</div>
</div>

{% endblock %}

