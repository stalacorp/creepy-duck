{% block javascripts -%}
    {{ parent() }}
    <script>
        var count = 0;
        var clicked = '';
        var clickedquality;        
        $(function () {

            $("form").submit(function (e) {
                if (clicked == 'intopeople_databasebundle_cdp_saveAndAdd') {

                    var teller = 1;
                    var okteller = 0;
                    $('.qualityrequired').each(function () {
                            var button = $(this).find('button');
                            if ($('#intopeople_databasebundle_cdp_coreQuality' + teller).val() === '') {
                                $(button).removeClass('btn-primary');
                                $(button).addClass('btn-danger');
                                okteller++;
                        }
                        teller++;

                    });

                    teller = 1;
                    $('.challengerequired').each(function () {
                        var button = $(this).find('button');
                        if ($('#intopeople_databasebundle_cdp_challenge' + teller).val() === '') {
                            $(button).removeClass('btn-primary');
                            $(button).addClass('btn-danger');
                            okteller++;
                        }
                        teller++;

                    });

                    teller = 1;
                    $('.pitfallrequired').each(function () {
                        var button = $(this).find('button');
                        if ($('#intopeople_databasebundle_cdp_pitfall' + teller).val() === '') {
                            $(button).removeClass('btn-primary');
                            $(button).addClass('btn-danger');
                            okteller++;
                        }
                        teller++;

                    });

                    if (okteller == 0) {

                    } else {
                        e.preventDefault();
                        $('#thefocus').focus();
                    }

                }

            });


            $("[type=submit]").click(function () {
                clicked = this.id;
            });

            $(document).on('click', '.qualityoption', function () {
                $('#corequalitiesModal').modal('hide');
                $(clickedquality).html($(this).text());
                $(clickedquality).removeClass('btn-primary');
                $(clickedquality).removeClass('btn-danger');
                $(clickedquality).addClass('btn-success');
                $('#intopeople_databasebundle_cdp_coreQuality' + $(clickedquality).data('id')).val($(this).text());

            });

            $(document).on('click', '.pitfalloption', function () {
                $('#pitfallsModal').modal('hide');
                $(clickedquality).html($(this).text());
                $(clickedquality).removeClass('btn-primary');
                $(clickedquality).removeClass('btn-danger');
                $(clickedquality).addClass('btn-success');
                $('#intopeople_databasebundle_cdp_pitfall' + $(clickedquality).data('id')).val($(this).text());

            });

            $(document).on('click', '.challengeoption', function () {
                $('#challengesModal').modal('hide');
                $(clickedquality).html($(this).text());
                $(clickedquality).removeClass('btn-primary');
                $(clickedquality).removeClass('btn-danger');
                $(clickedquality).addClass('btn-success');
                $('#intopeople_databasebundle_cdp_challenge' + $(clickedquality).data('id')).val($(this).text());

            });

            $(document).on('click', '.buttonquality', function () {
                clickedquality = $(this);
            });



            $(document).on('click', '#custombutton', function () {
                var text = $('#custom').val();
                if (text != '') {
                    $('#corequalitiesModal').modal('hide');
                    $(clickedquality).html(text);
                    $(clickedquality).removeClass('btn-primary');
                    $(clickedquality).removeClass('btn-danger');
                    $(clickedquality).addClass('btn-success');
                    $('#intopeople_databasebundle_cdp_coreQuality' + $(clickedquality).data('id')).val(text);
                }
            });

            $(document).on('click', '#custompitfallbutton', function () {
                var text = $('#custompitfall').val();
                if (text != '') {
                    $('#pitfallsModal').modal('hide');
                    $(clickedquality).html(text);
                    $(clickedquality).removeClass('btn-primary');
                    $(clickedquality).removeClass('btn-danger');
                    $(clickedquality).addClass('btn-success');
                    $('#intopeople_databasebundle_cdp_pitfall' + $(clickedquality).data('id')).val(text);
                }
            });

            $(document).on('click', '#customchallengebutton', function () {
                var text = $('#customchallenge').val();
                if (text != '') {
                    $('#challengesModal').modal('hide');
                    $(clickedquality).html(text);
                    $(clickedquality).removeClass('btn-primary');
                    $(clickedquality).removeClass('btn-danger');
                    $(clickedquality).addClass('btn-success');
                    $('#intopeople_databasebundle_cdp_challenge' + $(clickedquality).data('id')).val(text);
                }
            });


            $(document).on('click', '.addButton', function () {
                $(this).parent().parent().parent().find('.hidden').first().removeClass('hidden');

            });

            $(document).on('click', '.removeButton', function () {
                $(this).parent().parent().addClass('hidden');
            });
        });

    </script>
{% endblock %}

