                        <tr>
                            <th>{{ template.name }}:
                            <th>
                            <td>{{ entity.feedbackcycle.user }}</td>
                        </tr>

                        <tr>
                            <th>{{ template.supervisor }}:
                            <th>
                            <td>{{ entity.supervisor }}</td>
                        </tr>

                        <tr>
                            <th>{{ template.dateDiscussion }}:
                            <th>
                            <td>{{ entity.feedbackdate | localizeddate('medium', 'none', app.request.locale ) }}</td>
                        </tr>

                    </table>

                </div>

                <div class="col-md-6">

                    <table class="table borderless">

                        <tr>
                            <th>{{ template.team }}:
