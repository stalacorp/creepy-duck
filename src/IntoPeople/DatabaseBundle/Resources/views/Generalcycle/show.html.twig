{% extends '::base.html.twig' %} {% block body -%}
<!-- BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM-->
<div id="portlet-config" class="modal hide">
	<div class="modal-header">
		<button data-dismiss="modal" class="close" type="button"></button>
		<h3>Widget Settings</h3>
	</div>
	<div class="modal-body">Widget settings form goes here</div>
</div>
<div class="clearfix"></div>
<div class="container">
	<div class="content">

		<div class="page-title">
			<i class="icon-custom-left"></i>
			<h3>
				<span class="semi-bold">Generalcycles</span>
			</h3>
		</div>



						
								<div class="row">
			<div class="col-md-6">
				
				
				        <h5>Started by</h5>
				        <p>{{ entity.startedby }}</p>

				        <h5>Start date</h5>
                        <p>{{ entity.startdate|date('Y-m-d') }}</p>
    
                        <h5>Year</h5>                    
                        <p>{{ entity.year }}</p>
                        
                        
                        <h4 class="margin-top-20">Career development plan</h4>
                        
                        <div class="row form-row margin-top-20">                     
                        
                        <div class="col-md-6">
                        {{ entity.startdatecdp|date('Y-m-d H:i:s') }}
                        </div>
                        
                        <div class="col-md-6">
                        {{ entity.enddatecdp|date('Y-m-d H:i:s') }}
                        </div>
                        
                        </div>
                        
                        <h4 class="margin-top-20">Mid year</h4>
                        
                        <div class="row form-row margin-top-20">
                        
                        <div class="col-md-6">
                        {{ entity.startdatemidyear|date('Y-m-d H:i:s') }}
                        </div>
                        
                        <div class="col-md-6">
                        {{ entity.enddatemidyear|date('Y-m-d H:i:s') }}
                        </div>
                        
                        </div>
                        
                        <h4 class="margin-top-20">Year end</h4>
                        
                        <div class="row form-row margin-top-20">
                        
                        <div class="col-md-6">
                        {{ entity.startdateyearend|date('Y-m-d H:i:s') }}
                        </div>
                                             
                        <div class="col-md-6">
                        {{ entity.enddateyearend|date('Y-m-d H:i:s') }}
                        </div>
                        
                        </div>
                        
                      
                        
                        <ul class="record_actions">
							<li><a href="{{ path('generalcycle') }}"> Back to the list </a></li>
							<li><a
								href="{{ path('generalcycle_edit', { 'id': entity.id }) }}">
									Edit </a></li>
							<li>{{ form(delete_form) }}</li>
						</ul>
                                                  
					
			</div>
		</div>
					


	</div>
</div>

{% endblock %}

