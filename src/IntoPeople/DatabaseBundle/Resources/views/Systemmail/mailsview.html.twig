<table class="records_list">
    <thead>
    <tr>
        <th>Name</th>
        <th>Isactive</th>
        <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('systemmail_show', { 'id': entity.id }) }}">{{ entity.name }}</a></td>
            <td>{{ entity.sender }}</td>
            <td>{{ entity.isActive }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('systemmail_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('systemmail_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

