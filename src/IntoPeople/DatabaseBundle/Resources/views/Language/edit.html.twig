{% extends '::base.html.twig' %}

{% block body %}

    <h2>{{ 'language.edit.title' }}</h2>

    <div class="row">
        <div class="col-md-4">

            {{ form_start(edit_form) }}

            {{ form_row(edit_form.name) }}

            {{ form_widget(edit_form.submit) }}

            <a href="{{ path('language') }}" class="btn btn-default btn-cons">{{ 'button.back' | trans }}</a>

            {{ form_end(edit_form) }}


            <p>{{ form(delete_form) }}</p>

        </div>

    </div>


{% endblock %}
